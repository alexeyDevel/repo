{
    "version": 2,
    "name": "sovtrud-server",
    "env": {
        "DATABASE_URL": "@database_url",
        "JWT_KEY": "@jwt_key",
        "DIRECT_URL": "@direct_url"
    },
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.ts",
        "methods": ["GET", "POST", "PUT", "DELETE"]
      }
    ]
}